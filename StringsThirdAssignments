import edu.duke.*;
import java.io.*;

public class Part1 {
    public int findStopCodon(String dna, int startIndex, String stopCodon){
        int stopIndex = dna.indexOf(stopCodon,startIndex + 3);
        while(stopIndex != -1){
            if((stopIndex - startIndex) % 3 == 0){
                return stopIndex;
            }
            else{
                stopIndex = dna.indexOf(stopCodon, stopIndex + 1);
            }
        }
        return dna.length();  
    }
    
    public String findGene(String dna){
        int startIndex = dna.indexOf("ATG");
        if(startIndex == -1){
            return " ";
        }
        int taaIndex = findStopCodon(dna, startIndex, "TAA");
        int tagIndex = findStopCodon(dna, startIndex, "TAG");
        int tgaIndex = findStopCodon(dna, startIndex, "TGA");
        int temp = Math.min(taaIndex, tagIndex);
        int minIndex = Math.min(temp, tgaIndex);
        if(minIndex != dna.length()){
            String gene = dna.substring(startIndex, minIndex + 3);
            return gene;
        }
        else{
            return " ";
        }
    }
    //change
    public StorageResource getAllGenes(String dna){
        StorageResource geneList = new StorageResource();
        String gene = " ";
        String remain = dna;
        int remainIndex = 0;
        int stopIndex = 0;
        int indexATG = 0;
        int remainStart = 0;
        int COUNT = 0;
        while(true){   
            //test4
            gene = findGene(remain);
            System.out.println("The gene of dna4 is " + gene);
            if(!gene.equals(" ")){
                geneList.add(gene);
                remainIndex = remain.indexOf(gene);
                remainStart = remainIndex + gene.length();
                remain = remain.substring(remainStart, remain.length());
                COUNT = COUNT + 1;
            }
            else{
                indexATG = remain.indexOf("ATG") + 3;
                remainIndex = remain.indexOf("ATG", indexATG);
                if(remainIndex != -1){
                    remain = remain.substring(remainIndex, remain.length());
                }
            }
            if(remainIndex == -1){
                break;
            }
            System.out.println("remain is " + remain);
            System.out.println("length of remain is" + remain.length());
   
        }
        return geneList;
    }
    //change
    public float cgRatio(String dna){
        int count = 0;
        int pos = 0;
        int all = dna.length();
        int posplus1 = 0;
        String character;
        while(true){
            posplus1 = pos + 1;
            character = dna.substring(pos, posplus1);
            //System.out.println("character is " + character);
            if((character.equals("C")) || (character.equals("G"))){
            count = count + 1;
            //System.out.println("count is " + count); 
            }
            pos = pos + 1;
            if(pos == all){
                break;
            }
        }
         float ratio = (float)count / all;
         return ratio;
    }
    public int findCTG(String dna){
        int count = 0;
        int startIndex = 0;
        while(dna.indexOf("CTG",startIndex) != -1){
            startIndex = dna.indexOf("CTG",startIndex) + 3;
            count = count + 1;
        }
        return count;
    }
    
    public void processGenes(StorageResource sr){
        int count = 0;
        int count1 = 0;
        int count2 = 0;
        int longest = 0;
        float ratio;
        System.out.println("The number of genes is " + count2);
        for(String s:sr.data()){
            count2 = count2 + 1;
        }
        System.out.println("The number of genes is " + count2);
        for(String s:sr.data()){
            if(s.length() > 60){
                System.out.println("Strings in sr that are longer than 60 characters include " + s);
                count = count + 1;
            }   
        } 
        System.out.println("The number of strings in sr that are longer than 60 characters is " + count);
        for(String s:sr.data()){
            ratio = cgRatio(s);
            if(ratio > 0.35){
                System.out.println("Strings in sr whose C-G-ratio is higher than 0.35 include" + s);
                count1 = count1 + 1;
            }
        } 
        System.out.println("The number of strings whose ratio is higher than 0.35 is" + count1);
        for(String s:sr.data()){
            if(s.length() > longest){
                longest = s.length();
            } 
        }
        System.out.println("The length of the longest gene is " + longest);
    }
    
    public void testFindStopCodon(){
        String dna = "GGGAAAAAATAA";
        //String dna4 = "";
        int startIndex = dna.indexOf("ATG");
        String stopCodon = "TAA";
        int stopIndex = findStopCodon(dna, startIndex, stopCodon);
        System.out.println("The stop index of dna is " + stopIndex);
        System.out.println("the length of dna4 is " + dna.length());
        
    }
    
    public void testFindGene(){
        String dna4 = "";
        String gene4 = findGene(dna4);
        System.out.println("The gene of dna4 is " + gene4);
        StorageResource genes = getAllGenes(dna4);
        for(String g:genes.data()){
            System.out.println("genelist is " + g);    
        }
        
        String dna5 = "ATGCCATAG";
        float ratio = cgRatio(dna5);
        System.out.println("The ratio of C and G is " + ratio);
                
    }
    
    public void testCTG(){
        String dna = "ACTGAACTGAAACTGA";
        int count = findCTG(dna);
        System.out.println("The number of CTG is" + count);
    }
    
    public void testProcessGenes(){
        //String dna4 = "";
        FileResource fr = new FileResource("fromweb.fa");
        String dna5 = fr.asString();
        dna5 = dna5.toUpperCase();
        int count = findCTG(dna5);
        System.out.println("The number of CTG equals " + count);
        System.out.println("dna5 equals" + dna5);
        StorageResource genes = getAllGenes(dna5);
        for(String g:genes.data()){
            System.out.println("genelist is " + g);    
        }
        processGenes(genes);   
    }
}
